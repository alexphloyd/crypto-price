{
  "name": "shared-utility-types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared-utility-types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared-utility-types",
        "main": "libs/shared-utility-types/src/index.ts",
        "tsConfig": "libs/shared-utility-types/tsconfig.lib.json",
        "assets": ["libs/shared-utility-types/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared-utility-types/**/*.ts", "libs/shared-utility-types/package.json"]
      }
    }
  },
  "tags": []
}
